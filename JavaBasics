// print numbers from 1 to n
/* import java.util.*;
public class JavaBasics {
  public static void main(String args[]) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int count = 1;
    while (count < n) {
      System.out.println(count);
      count++;
    }
  }
} */

// print sum of first n natural numbers using while loop
/* 
import java.util.*;
public class JavaBasics {
  public static void main(String args[]) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int sum = 0;
    int i = 1;
    while (i <= n) {
      sum += i;
      i++;
    }   
    System.out.println("Sum is: " + sum);
}
} */

// for  loops
// print sum of 1 to n using for loop
/* import java.util.*;
public class JavaBasics {
    public static void main(String args[]){
      Scanner sc = new Scanner(System.in);
      int n = sc.nextInt();
      int sum = 0;
      for (int i = 1; i <= n; i++) {
        sum += i;
      }
      System.out.println("Sum is: " + sum);
}
} */

